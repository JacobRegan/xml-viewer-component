{
  "name": "xml-viewer-component",
  "version": "0.1.9",
  "description": "Xml Viewer Component - a simple XML prettifier",
  "keywords": [
    "xml",
    "web-component",
    "xml-viewer",
    "view xml",
    "pretiffy xml",
    "xml prettifier"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "browser": "dist/xmlviewercomponent.js",
  "scripts": {
    "build": "stencil build",
    "build-es5": "stencil build --es5",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "stencil test --e2e",
    "test.watch": "jest --watch --no-cache"
  },
  "dependencies": {},
  "devDependencies": {
    "@stencil/core": "1.0.4",
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/jest": "23.3.14",
    "@types/puppeteer": "1.12.4",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "puppeteer": "^1.17.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dwarfcoder/xml-viewer-component.git"
  },
  "author": "Aleks Solntseff",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dwarfcoder/xml-viewer-component"
  },
  "homepage": "https://github.com/dwarfcoder/xml-viewer-component",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
